{
  "name": "essence",
  "version": "0.0.1",
  "description": "Essence UI for React",
  "private": false,
  "main": "dist/npm/es5/index.js",
  "jsnext:main": "next.js",
  "typings": "typings/index.d.ts",
  "scripts": {
    "start": "cd site && node run.js",
    "typescript-test": "tsc --project typings/typing-tests",
    "test": "flow check && BABEL_ENV=development jest",
    "jest": "jest",
    "lint": "eslint src libs site --quiet",
    "build": "cd build/site && webpack --progress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/essencemike/Essence.git"
  },
  "keywords": [
    "React",
    "UI",
    "react-components"
  ],
  "author": "IMike",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/essencemike/Essence/issues"
  },
  "homepage": "https://github.com/essencemike/Essence#readme",
  "dependencies": {
    "async-validator": "^1.8.2",
    "classnames": "^2.2.5",
    "font-awesome": "^4.7.0",
    "prop-types": "^15.6.1",
    "raf": "^3.4.0",
    "react-click-outside": "^3.0.1",
    "throttle-debounce": "^1.0.1"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@types/react": "^16.3.11",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "babel-standalone": "^6.26.0",
    "codemirror": "^5.36.0",
    "core-js": "^2.5.5",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.46.2",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "flow-bin": "^0.69.0",
    "gulp": "^3.9.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-postcss": "^7.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "marked": "^0.3.19",
    "node-sass": "^4.8.3",
    "postcss-flexbugs-fixes": "^3.3.0",
    "postcss-loader": "^2.1.4",
    "postcss-salad": "^2.0.1",
    "prismjs": "^1.14.0",
    "raw-loader": "^0.5.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-hot-loader": "^4.0.1",
    "react-scroll-up": "^1.3.3",
    "regenerator-runtime": "^0.11.1",
    "sass-loader": "^7.0.1",
    "sinon": "^4.5.0",
    "style-loader": "^0.20.3",
    "typescript": "^2.8.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-dev-server": "^3.1.3"
  },
  "jest": {
    "testRegex": "(/jest/.+\\.(js|jsx)$)|_test.jsx?$",
    "testPathIgnorePatterns": [
      "<rootDir>/tests",
      "<rootDir>/dist"
    ],
    "moduleNameMapper": {
      "\\.(png|eot|svg|ttf|woff|woff2)(\\?.+)?$": "<rootDir>/tests/jest/mock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/tests/jest/config.js"
  }
}
